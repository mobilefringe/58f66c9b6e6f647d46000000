<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            var jobs_list = getJobsList();
            console.log(jobs_list)
            var jobs = [];
            $.each(jobs_list, function(key, val){
                if(val.is_coming_soon_store === true){
                    jobs.push(val);
                } 
            });
            if(jobs.length > 0){
                renderJobs('#jobs_container', '#jobs_template', jobs);
                $("#coming_soon").show();
            } else {
                $("#no_stores").show();
            }
        }
        loadMallData(renderAll); 
    });
</script>