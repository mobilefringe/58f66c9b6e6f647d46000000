<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            var prefix = get_prefix();
            var pages_json = prefix + "/pages/lansdowne-directions.json"
            $.getJSON(pages_json).done(function(data) {
                $('#mm_page_title').html(data.title);
                $('#mm_page_content').html(data.body);
            }).fail(function(jqXHR) {
                if (jqXHR.status == 404) {
                    $("#404_msg").fadeIn("fast");
                }
            }); 
        }
        loadMallDataCached(renderAll); 
    })
</script>